{
    "version": 2,
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!assets|public|favicon.ico).*|[^.]+)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 10
        }
    }
}